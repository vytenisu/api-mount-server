    "@types/express": "^4.17.6",
    "@types/fetch-mock": "^7.3.2",
    "@types/jest": "^25.1.4",
    "@types/node-fetch": "^2.5.7",
    "@types/param-case": "^1.1.2",
    "fetch-mock": "^9.10.1",
    "fetch-mock-jest": "^1.3.0",
    "find-node-modules": "^2.0.0",
    "jest": "^25.2.3",
    "license-webpack-plugin": "^2.1.4",
    "node-fetch": "^2.6.0",
    "npm-dts-webpack-plugin": "^1.2.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
